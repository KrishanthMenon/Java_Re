package com.capgemini.helloworld;

public class My_ifelse_ladder 
{
	public static void main(String args[])
	{
		int day=1;
		if(day==1)
		{
			System.out.println("Mon");
		}
		else if(day==2)
		{
			System.out.println("Tue");
		}
		else if(day==3)
		{
			System.out.println("Wed");
		}
		else
		{
			System.out.println("Unknown");
		}
	}

}
